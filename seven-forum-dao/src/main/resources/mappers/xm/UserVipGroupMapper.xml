<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.seven.forum.dao.xm.UserVipGroupDao">

    <!--根据vip组id查询vip组-->
    <select id="getUserVipGroupByGroupId" resultType="com.seven.forum.entity.xm.UserVipGroup">
        select *
        from nf_user_vip_group
        where vip_group_id = #{groupId}
    </select>

</mapper>