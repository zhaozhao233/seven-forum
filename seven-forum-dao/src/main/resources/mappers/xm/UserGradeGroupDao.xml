<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.seven.forum.dao.xm.UserGradeGroupDao">

    <!--根据等级组id查询等级组-->
    <select id="getUserGradeGroupByGroupId" resultType="com.seven.forum.entity.xm.UserGradeGroup">
        select *
        from nf_user_grade_group
        where grade_group_id = #{groupId}
    </select>

</mapper>