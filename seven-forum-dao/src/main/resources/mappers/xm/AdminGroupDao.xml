<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.seven.forum.dao.xm.AdminGroupDao">

    <!--根据管理员组id查询管理员组-->
    <select id="getAdminGroupByGroupId" resultType="com.seven.forum.entity.xm.AdminGroup">
        select *
        from seven_forum.nf_admin_group
        where admin_group_id = #{groupId}
    </select>

</mapper>